  "description": "A light-weight core banking application that powers banking operations.",
  "main": "server.js",
  "scripts": {
    "start": "babel-node server.js",
    "test": "nyc --reporter=html --reporter=text mocha --require babel-register --timeout 40000 ./tests/*.js --exit",
